#! /usr/bin/env bash

function print_usage () {
   echo "forksum help:"
   echo
   echo "SYNOPSIS:  Prints the output of 'forkname farm summary' for the specified fork."
   echo
   echo "PARAMETERS:"
   echo "    forkname            Required."
   echo "    -help | --help      Show this information again."
   echo
   exit 0
}

VALIDATEFORKNAME='Yes'
. ftinit.sh

cd $FORKTOOLSBLOCKCHAINDIRS/$1-blockchain 
. ./activate 
$1 farm summary
deactivate
echo

